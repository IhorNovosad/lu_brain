static Hnn_Table hnn_table_create(Mem mem, lu_size size_in_cells)
{
	lu_assert(mem);

	if (size_in_cells == 0)
		return NULL;

	Hnn_Table self = (Hnn_Table) mem_alloc(mem, sizeof(struct hnn_table));
	lu_assert(self);

	self->mem = mem;

	return self;
}

static void hnn_table_destroy(Hnn_Table self)
{
	lu_assert(self);

	Mem mem = self->mem;

	self->mem = NULL;
	mem_free(mem, (lu_p_byte) self);
}