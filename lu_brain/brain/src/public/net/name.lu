///////////////////////////////////////////////////////////////////////////////
// Init, create and destroy

	Name name_create(Brain brain)
	{
		Name self 			= (Name) malloc(sizeof(struct name));

		// Initialize 
		self->brain 		= brain;

		return self;
	}

	void name_destroy(Name self)
	{
		lu_free((void**) self);
	}


