///////////////////////////////////////////////////////////////////////////////
// Init, create and destroy

	static Lu_Wave wave_init(Lu_Wave self, Gate gate, S_Mem s_mem, N_Mem n_mem)
	{
		lu_user_assert(gate, "Gate is NULL");
		lu_user_assert(s_mem, "S_Mem is NULL");
		lu_user_assert(n_mem, "N_Mem is NULL");

		self->gate 		= gate;
		self->s_mem 	= s_mem;
		self->n_mem 	= n_mem;

		return self;
	}

