
///////////////////////////////////////////////////////////////////////////////
// N_Mem

	N_Mem n_mem_create(Brain brain)
	{
		N_Mem self 			= (N_Mem) malloc(sizeof(struct n_mem));

		self->brain 		= brain;

		return self;
	}

	void n_preallocated_mem_destroy(N_Mem* self)
	{
		lu_free((void**) self);
	}