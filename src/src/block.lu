///////////////////////////////////////////////////////////////////////////////
// Init, create and destroy

	Block block_create(Story story)
	{
		Block self 			= (Block) malloc(sizeof(struct block));

		// Initialize
		self->story 		= story;

		return self;
	}

	void block_destroy(Block* self)
	{
		lu_free((void**) self);
	}


